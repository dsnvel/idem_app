<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IDEM.ListPage"
             Title="Список секретов"
             BackgroundColor="White">
    <ScrollView Grid.Row="10" Grid.ColumnSpan="1">
        <Grid  RowDefinitions="*, *, *, *, *, *, *, *, *, *">
            <Button x:Name="Stars" Text="De Sterrennacht" Clicked="DeSterrennachtOnClicked"
                    Grid.Column="0" Grid.Row="0" BackgroundColor="Black" TextColor="White" IsVisible="True"/>
        </Grid>
    </ScrollView>
</ContentPage>